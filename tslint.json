{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "forin": false,
    "function-name": false,
    "max-classes-per-file": false,
    "no-shadowed-variable": false,
    "no-empty": false,
    "member-ordering": false,
    "member-access": false,
    "curly": false,
    "import-name": false,
    "ter-arrow-parens": false,
    "variable-name": false,
    "object-shorthand-properties-first": false,
    "prefer-array-literal": false,
    "no-console": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ],
    "quotemark": [
      true,
      "double"
    ]
  }
}